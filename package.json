{
  "name": "mcalendar",
  "version": "1.0.0",
  "description": "Pure JS event calendar",
  "main": "config/webpack.config.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config ./config/webpack.dev.js",
    "prettier": "prettier --config .prettierrc --write src/scss/**/*.scss src/js/**/*.js",
    "lint": "npm run lint:style && npm run lint:js",
    "lint:style": "stylelint src/scss/**/*.scss --config .stylelintrc --fix",
    "lint:js": "./node_modules/.bin/eslint -c .eslintrc src/js/**/*.js -f compact --fix",
    "build": "./node_modules/.bin/webpack --config ./config/webpack.prod.js",
    "bundle": "npm run prettier && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/llooyydd2004/mCalendar.git"
  },
  "keywords": [
    "calendar",
    "js",
    "event",
    "html"
  ],
  "author": "Martin Blumentritt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/llooyydd2004/mCalendar/issues"
  },
  "homepage": "https://github.com/llooyydd2004/mCalendar#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "babel": "^6.23.0",
    "copy-webpack-plugin": "^9.0.1",
    "lint": "^0.7.0",
    "prettier": "^2.3.2",
    "webpack-4.17.1": "^4.17.1",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "webpack": "^4.46.0"
  }
}
